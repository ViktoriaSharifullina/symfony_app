{% embed 'shared/_tabs.html.twig' %}
	{% block tabs %}
		{% embed 'shared/_tab.html.twig' with { route: 'app_micro_post', classes: ""  } %}
			{% block path %}
				{{ path('app_micro_post') }}
			{% endblock %}
			{% block content %}
				All Posts
			{% endblock %}
		{% endembed %}

		{% embed 'shared/_tab.html.twig' with { route: 'app_micro_post_topliked', classes: "ml-4" } %}
			{% block path %}
				{{ path('app_micro_post_topliked') }}
			{% endblock %}
			{% block content %}
				Top Liked
			{% endblock %}
		{% endembed %}

		{% embed 'shared/_tab.html.twig' with { route: 'app_micro_post_follows', classes: "ml-4"  } %}
			{% block path %}
				{{ path('app_micro_post_follows') }}
			{% endblock %}
			{% block content %}
				From People You Follow
			{% endblock %}
		{% endembed %}
	{% endblock %}
{% endembed %}
